<h3>About</h3>

<table>
    <tbody >
        <tr>
            <th>@AboutDetails.FullName.Key</th>
            <td>@AboutDetails.FullName.Value</td>
        </tr>
        <tr>
            <th>@AboutDetails.DateOfBirth.Key</th>
            <td>@AboutDetails.DateOfBirth.Value</td>
        </tr> 
         <tr>
            <th>@AboutDetails.Nationality.Key</th>
            <td>@AboutDetails.Nationality.Value</td>
        </tr>
        <tr>
            <th>@AboutDetails.Email.Key</th>
            <td>@AboutDetails.Email.Value</td>
        </tr>
        <tr>
            <th>@AboutDetails.GitHubLink.Key</th>
            <td><a href="@AboutDetails.GitHubLink.Value">GitHub Link</a></td>
        </tr>
        <tr>
            <th>@AboutDetails.LinkedInLink.Key</th>
            <td><a href="@AboutDetails.LinkedInLink.Value">LinkedIn Link</a></td>
        </tr>
        <tr>
            <th>@AboutDetails.PersonalStatement.Key</th>
            <td>@AboutDetails.PersonalStatement.Value</td>
        </tr>
    </tbody>
</table>


