<h3>About</h3>

<table>
    <tbody >
        <tr>
            <th>Full Name</th>
            <td>@AboutDetails.FullName</td>
        </tr>
        <tr>
            <th>DOB</th>
            <td>@AboutDetails.DateOfBirth</td>
        </tr>
        <tr>
            <th>Nationality</th>
            <td>@AboutDetails.Nationality</td>
        </tr>
        <tr>
            <th>Email</th>
            <td>@AboutDetails.Email</td>
        </tr>
        <tr>
            <th>GitHub</th>
            <td><a href="@AboutDetails.GitHubLink">GitHub Link</a></td>
        </tr>
        <tr>
            <th>LinkedIn</th>
            <td><a href="@AboutDetails.LinkedInLink">LinkedIn Link</a></td>
        </tr>
    </tbody>
</table>
