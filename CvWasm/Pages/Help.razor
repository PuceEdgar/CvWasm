<h3>Help</h3>

@foreach (KeyValuePair<string, string> command in AvailableCommands)
{
    <p>@command.Key - @command.Value</p>
}

@code {
    private readonly Dictionary<string, string> AvailableCommands = new()
    {
            [AboutCommand] = "Shows about page",
            [ExperienceCommand] = "Shows work experience page",
            [HardSkillsCommand] = "Shows skills page",
            [EducationCommand] = "Shows education page",
            [HelpCommand] = "Shows help page",
            [NavigationCommand] = "Shows possible navigation options",
            [HomeCommand] = "Go back to main page",
            [ClearCommand] = "Clear window",
            [OpenGitHubCommand] = "Opens Github profile in new tab",
            [OpenLinkedInCommand] = "Opens LinkedIn profile in new tab"
    };
}
